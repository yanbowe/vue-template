{
  "name": "vue-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:lint-staged": "lint-staged",
    "commit": "git pull && git add -A && git-cz && git push"
  },
  "dependencies": {
    "@unocss/reset": "^0.45.30",
    "@vueuse/core": "^9.6.0",
    "axios": "^1.2.1",
    "pinia": "^2.0.27",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@sxzz/eslint-config": "^2.4.4",
    "@types/node": "^18.11.11",
    "@vitejs/plugin-vue": "^3.2.0",
    "commitizen": "^4.2.6",
    "cz-git": "^1.4.0",
    "eslint": "^8.29.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "typescript": "^4.9.4",
    "unocss": "^0.45.30",
    "unplugin-auto-import": "^0.11.5",
    "unplugin-vue-components": "^0.22.11",
    "vite": "^3.2.5",
    "vue-tsc": "^0.40.13"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": true
    }
  },
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix"
  }
}
