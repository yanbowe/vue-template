{
  "name": "vue-template",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:lint-staged": "lint-staged"
  },
  "dependencies": {
    "@unocss/reset": "^0.45.30",
    "@vueuse/core": "^9.4.0",
    "axios": "^1.1.3",
    "pinia": "^2.0.23",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@sxzz/eslint-config": "^2.4.4",
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "^3.2.0",
    "commitizen": "^4.2.5",
    "cz-git": "^1.3.12",
    "eslint": "^8.27.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "typescript": "^4.8.4",
    "unocss": "^0.45.30",
    "unplugin-auto-import": "^0.11.4",
    "unplugin-vue-components": "^0.22.9",
    "vite": "^3.2.2",
    "vue-tsc": "^0.40.13"
  },
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": true
    }
  }
